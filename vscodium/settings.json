{
  "git.enableSmartCommit": true,
  "codesnap.backgroundColor": "#1e1e2e",
  "codesnap.realLineNumbers": true,
  "codesnap.showWindowTitle": true,
  "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 10px 34px",
  "git.enableCommitSigning": true,
  "vscord.status.problems.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "vscord.app.name": "VSCodium",
  "vscord.status.state.text.editing": "Working on {file_name}{file_extension}",
  "vscord.status.idle.enabled": false,
  "vscord.status.idle.check": false,
  "css.lint.unknownAtRules": "ignore",
  "prettier.printWidth": 150,
  "svelte.enable-ts-plugin": true,
  "go.toolsManagement.autoUpdate": true,
  "editor.fontFamily": "'Fira Code'",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.productIconTheme": "bongocat",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.requireConfig": true,
  "editor.formatOnSave": true,
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.gpuAcceleration": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "codesnap.shutterAction": "copy",
  "workbench.startupEditor": "none",
  "editor.fontLigatures": true,
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "workbench.colorTheme": "Catppuccin Mocha",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "[astro]": {
    "editor.defaultFormatter": "astro-build.astro-vscode"
  },
  "debug.disassemblyView.showSourceCode": false,
  "workbench.sideBar.location": "right",
  "files.associations": {
    "*.service": "ini"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "editor.tabSize": 2,
  "diffEditor.ignoreTrimWhitespace": false
}