{
  "git.enableSmartCommit": true,
  "codesnap.backgroundColor": "#1e1e2e",
  "codesnap.realLineNumbers": true,
  "codesnap.showWindowTitle": true,
  "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 10px 34px",
  "git.enableCommitSigning": true,
  "gpgIndicator.enablePassphraseCache": true,
  "vscord.status.problems.enabled": false,
  "editor.minimap.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "vscord.app.name": "VSCodium",
  "vscord.status.state.text.editing": "Working on {file_name}{file_extension}",
  "vscord.status.idle.enabled": false,
  "vscord.status.idle.check": false,
  "css.lint.unknownAtRules": "ignore",
  "prettier.printWidth": 150,
  "svelte.enable-ts-plugin": true,
  "go.toolsManagement.autoUpdate": true,
  "editor.fontFamily": "'Fira Code'",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.productIconTheme": "bongocat",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.requireConfig": true,
  "editor.formatOnSave": true,
  "markdown.extension.preview.autoShowPreviewToSide": true
}